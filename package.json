{
  "name": "webdriverio-cucumber-typescript",
  "version": "0.0.1",
  "author": "MIT",
  "scripts": {
    "build": "npx del ./dist && npx tsc && npx copyfiles -f ./src/test/e2e/features/** ./dist/src/test/e2e/features/",
    "watch": "npx tsc -w",
    "code:format": "eslint . --ext .ts && prettier --check \"src/**/*.ts*\" --write",
    "commit": "cz",
    "clean-mac": "npx del test-report",
    "clean": "npx rimraf test-report",
    "test1": "npm run build && npm run tests:e2e",
    "onPhone1": "npm run build && npm run android-conf",
    "android-conf": "npx del test-report &&  wdio ./configs/androidBrowser.config.js",
    "tests:e2e": "wdio ./configs/wdio.conf.js",
    "report:generate": "npx allure generate --clean ./test-report/allure-result/ -o ./test-report/allure-report",
    "report:open": "npx allure open test-report/allure-report",
    "test": "npm-run-all clean tests:e2e report:generate report:open --continue-on-error",
    "test-local:onPhone": "npm run android-conf",
    "conf:multi": "wdio ./configs/wdio.parallel.conf.js",
    "test-multiBrowser": "npm-run-all clean conf:multi report:generate report:open --continue-on-error"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@wdio/allure-reporter": "^7.14.1",
    "@wdio/cli": "^7.14.1",
    "@wdio/cucumber-framework": "^7.14.1",
    "@wdio/local-runner": "^7.14.1",
    "@wdio/spec-reporter": "^7.14.1",
    "allure-commandline": "^2.13.8",
    "aws-sdk": "^2.999.0",
    "chromedriver": "^94.0.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^3.0.1",
    "dotenv": "^10.0.0",
    "expect": "^26.6.2",
    "expect-webdriverio": "^3.1.4",
    "fibers": "^5.0.0",
    "multiple-cucumber-html-reporter": "^1.16.2",
    "npm-run-all": "^4.1.5",
    "wdio-chromedriver-service": "^7.2.0",
    "wdio-rerun-service": "^1.7.2",
    "webdriverio": "^7.14.1",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@cucumber/cucumber": "^7.3.1",
    "@types/jquery": "^3.5.5",
    "@types/node": "^15.14.7",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@wdio/appium-service": "^7.14.1",
    "@wdio/selenium-standalone-service": "^7.14.1",
    "appium": "^1.21.0",
    "chai": "^4.3.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "husky": "^6.0.0",
    "prettier": "^2.3.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all code:* build cucumber:*:step",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
